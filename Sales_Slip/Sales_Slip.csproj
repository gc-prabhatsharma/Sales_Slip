<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="C1.Win.FlexGrid" Version="6.0.20221.548" />
    <PackageReference Include="C1.Win.FlexGrid.BandedView" Version="6.0.20221.548-beta" />
    <PackageReference Include="C1.Win.FlexGrid.ImportExport" Version="6.0.20221.548" />
    <PackageReference Include="C1.Win.Themes" Version="6.0.20221.548" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="3.1.9" />
    <PackageReference Include="Microsoft.Data.Sqlite.Core" Version="3.1.9" />
  </ItemGroup>

  <ItemGroup>
    <None Update="SalesData.mdb">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>